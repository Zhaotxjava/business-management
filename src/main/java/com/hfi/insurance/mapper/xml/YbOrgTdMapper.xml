    <sql id="torgTdbxListWhere">
        <where>
            <if test="hospitalid!=null and hospitalid!=''">
                AKB020 = #{hospitalid}
            </if>
            <if test="number!=null  and number!=''">
                and  AKB020 like concat("%",#{number},"%")
            </if>
            <if test="institutionName!=null and institutionName!=''">
                and AKB021  like concat("%",#{institutionName},"%")
            </if>

        </where>
    </sql>

    <select id="getorgTdbxList"  parameterType="com.hfi.insurance.model.dto.InstitutionInfoQueryReq" resultType="com.hfi.insurance.model.YbOrgTd">

     SELECT  * from  yb_org_td
     <include refid="torgTdbxListWhere"/>
     limit #{pageNum},#{pageSize}
    </select>

    <select id="selectorgTdbxCount"  parameterType="com.hfi.insurance.model.dto.InstitutionInfoQueryReq" resultType="int">
     SELECT  count(*) from  yb_org_td
     <include refid="torgTdbxListWhere"/>
    </select>

