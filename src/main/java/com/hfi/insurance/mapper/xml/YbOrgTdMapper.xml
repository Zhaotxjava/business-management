    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        AKB020, AKB021, AKB022, AAE053, AKA101, AKB023, BKB012, AAE045, BKB001, AAE100, BKB117, AAE013, BKB301, BKB302, BKA938, AAE004, AAE005, AAE006, BKA968, BKA975, BKA977, BKA978, BKA983, AAA027, BKA996, AKB141, BKA992, BKB500, BKB501, BKB502, BKB503, BKB504, BKB505
